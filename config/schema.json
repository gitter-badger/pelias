{

  "settings": {

    "analysis": {

      "analyzer": {

        "suggestions": {
          "type": "custom",
          "tokenizer": "whitespace",
          "filter": "lowercase"
        }

      }

    },

    "index": {
      "number_of_replicas": "0",
      "number_of_shards": "8",
      "index.index_concurrency": "32"
    }

